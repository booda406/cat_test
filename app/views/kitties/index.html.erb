<h1>Listing kitties</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @kitties.each do |kitty| %>
      <tr>
        <td><%= link_to 'Show', kitty %></td>
        <td><%= link_to 'Edit', edit_kitty_path(kitty) %></td>
        <td><%= link_to 'Destroy', kitty, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kitty', new_kitty_path %>
